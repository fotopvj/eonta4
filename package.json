{
  "name": "eonta",
  "version": "2.0.0",
  "description": "A web-based compositional utility for immersive audio environments using GPS-based audio authoring",
  "main": "server/app.js",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
